%%BACK_SQL%%libexec/openldap/back_sql-2.2.so.7
%%MODULES%%@unexec rmdir %D/libexec/openldap 2>/dev/null || true
libexec/slapd
libexec/slurpd
@comment sbin/slapacl
sbin/slapadd
@comment sbin/slapauth
sbin/slapcat
sbin/slapdn
sbin/slapindex
sbin/slappasswd
sbin/slaptest
@exec mkdir -p %%LDAP_RUN_DIR%%
