#!/bin/sh
#
# $FreeBSD: /tmp/pcvs/ports/editors/openoffice-devel/files/Attic/openoffice-wrapper,v 1.2 2002-10-21 16:46:37 mbr Exp $

oopath=%%PREFIX%%/OpenOffice.org-643/program/
program=`echo $0 | sed -e 's|.*-||'`

case $program in
$0)
	exec $oopath/soffice "$@"
	;;
	*)
	exec $oopath/$program "$@"
	;;
esac
