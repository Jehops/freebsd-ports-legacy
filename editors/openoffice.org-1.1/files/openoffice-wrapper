#!/bin/sh
#
# $FreeBSD: /tmp/pcvs/ports/editors/openoffice.org-1.1/files/Attic/openoffice-wrapper,v 1.13 2004-07-31 03:29:15 maho Exp $

oopath=%%PREFIX%%/OpenOffice.org%%RELEASE_NR%%/program/
program=`echo $0 | sed -e 's|.*-%%ELEASE_NR%%-||'`

case $program in
$0)
	$oopath/soffice "$@"
	;;
	*)
	$oopath/$program "$@"
	;;
esac
