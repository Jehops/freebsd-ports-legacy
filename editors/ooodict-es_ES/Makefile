COMMENT=	Spanish (Spain) MySpell dictionary for OpenOffice.org

LANGPREFIX=	es
LANGSUFFIX=	ES
LANGDIST+=	es_ES.zip
HYPHDIST+=	hyph_es_ES.zip
THESDIST+=	thes_es_ES.zip
MASTERDIR=	${.CURDIR}/../ooodict-all
RDEP=		${PORTSDIR}/editors/openoffice.org-1.1

.include "${MASTERDIR}/Makefile"
