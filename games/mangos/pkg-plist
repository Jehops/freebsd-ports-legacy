bin/mangos-worldd
bin/mangos-realmd
@unexec if cmp -s %D/etc/mangosd.conf %D/etc/mangosd.conf.sample;then rm -f %D/etc/mangosd.conf;fi
etc/mangosd.conf.sample
@exec cp -n %D/%F %B/mangosd.conf
@unexec if cmp -s %D/etc/realmd.conf %D/etc/realmd.conf.sample;then rm -f %D/etc/realmd.conf;fi
etc/realmd.conf.sample
@exec cp -n %D/%F %B/realmd.conf
