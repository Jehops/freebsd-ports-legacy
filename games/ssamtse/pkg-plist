@unexec rm %D/ssam/Bin/libogg.so.0 2>&1 >/dev/null || true
@unexec rm %D/ssam/Bin/libGame.so 2>&1 >/dev/null || true
@unexec rm %D/ssam/Bin/libEntities.so 2>&1 >/dev/null || true
@dirrm ssam/Bin
@dirrm ssam
