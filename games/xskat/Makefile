DISTNAME=	xskat-1.6
CATEGORIES=	games
MASTER_SITES=	ftp://ftp.germany.eu.net/pub/X11/misc/

MAINTAINER=	tg@FreeBSD.org

WRKSRC=		${WRKDIR}/xskat
USE_IMAKE=	yes

pre-configure:
	cp ${FILESDIR}/Imakefile ${WRKSRC}
	mv ${WRKSRC}/makefile ${WRKSRC}/makefile.dist

.include <bsd.port.mk>
