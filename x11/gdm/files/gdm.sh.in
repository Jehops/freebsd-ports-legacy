#!/bin/sh
# $FreeBSD$

# PROVIDE: gdm
# REQUIRE: DAEMON
# KEYWORD: FreeBSD shutdown

gdm_enable=${gdm_enable-"NO"}

. %%RC_SUBR%%

name=gdm
rcvar=`set_rcvar`
command="%%PREFIX%%/bin/${name}"
pidfile="/var/run/${name}.pid"
procname="%%PREFIX%%/bin/gdm-binary"

load_rc_config ${name}
run_rc_command "$1"
