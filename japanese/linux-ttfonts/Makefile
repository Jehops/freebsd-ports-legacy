PORTNAME=		ttfonts-ja
PORTVERSION=		1.2
PORTREVISION=		5
CATEGORIES=		japanese x11-fonts linux
DISTNAME=	${PORTNAME}-${PORTVERSION}-36

MAINTAINER=	ports@FreeBSD.org
COMMENT=	Japanese Truetype fonts for use with Linux emulation

DEPRECATED=	"The linuxolator uses the fonts from the X-server or font-server, so there is no need to install fonts into LINUXBASE"
