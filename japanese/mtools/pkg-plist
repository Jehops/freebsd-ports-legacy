@exec ln -fs %D/%F %B/mcd
@exec ln -fs %D/%F %B/mcopy
@exec ln -fs %D/%F %B/mdel
@exec ln -fs %D/%F %B/mdeltree
@exec ln -fs %D/%F %B/mdir
@exec ln -fs %D/%F %B/mdu
