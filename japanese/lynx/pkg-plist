bin/lynx
etc/lynx.cfg.sample
@exec test -f %B/lynx.cfg && mv -f %B/lynx.cfg %B/lynx.oldcfg
@exec cp -f %D/%F %B/lynx.cfg
@unexec rm -f %B/lynx.cfg
@unexec test -f %B/lynx.oldcfg && mv -f %B/lynx.oldcfg %B/lynx.cfg
