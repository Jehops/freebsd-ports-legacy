@comment $FreeBSD$
etc/ebnetd.conf.sample
libexec/ebhtstat
libexec/ebnstat
libexec/ndtpstat
sbin/ebhtcheck
sbin/ebhtcontrol
sbin/ebhttpd
sbin/ebncheck
sbin/ebncontrol
sbin/ebndaily
sbin/ebnetd
sbin/ebnupgrade
sbin/ndtpcheck
sbin/ndtpcontrol
sbin/ndtpd
@exec mkdir -m 0755 -p %%EBNETD_RUNDIR%%
@unexec rmdir %%EBNETD_RUNDIR%%
