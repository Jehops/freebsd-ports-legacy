@unexec if [ -x %D/etc/rc.d/esecanna ]; then %D/etc/rc.d/esecanna stop || true; fi
etc/esecannarc
@exec mkdir -p %D/lib/esecanna
@dirrm lib/esecanna
sbin/esecannaserver
@exec /bin/mkdir -p /var/run/esecanna
@unexec /bin/rm -rf /var/run/esecanna
