@unexec if [ -f %B/canna.sh ]; then chmod +x %B/canna.sh; fi
@exec if [ -x %B/canna.sh ]; then chmod -x %B/canna.sh; fi
@exec mkdir -p %D/lib/esecanna
@dirrm lib/esecanna
