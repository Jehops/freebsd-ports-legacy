@unexec %%LOCALBASE%%/bin/update-mime-database %D/share/mime
@dirrmtry share/gvfs/mounts
@dirrmtry share/gvfs
@dirrmtry share/icons/gnome/48x48/apps
@dirrmtry share/icons/gnome/48x48
@dirrmtry share/icons/gnome
@dirrmtry share/icons
