#
# This is the header file for constructing make files for FreeBSD.
#

# Default flags to use when invoking the C compiler.

CFLAGS ?= -O2
CC ?= gcc

# Assembly language object files.

Aruntime = x86.o 
SAruntime = x86.so

# Profiled library

Plib = libsc_p.a

# Installation tools

RANLIB = ranlib
AS = as
CPP = cpp

# X library

XLIB = -L${X11BASE}/lib -lX11
XLIBCFLAGS =
PICFLAGS = -fpic -DPIC


# End of FREEBSD header.
