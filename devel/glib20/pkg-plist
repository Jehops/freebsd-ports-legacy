lib/libgmodule12.so.1
lib/libgthread12.a
lib/libgthread12.la
lib/libgthread12.so
lib/libgthread12.so.1
@unexec /usr/bin/env OBJFORMAT=%%PORTOBJFORMAT%% /sbin/ldconfig -R
@exec /usr/bin/env OBJFORMAT=%%PORTOBJFORMAT%% /sbin/ldconfig -m %B
share/aclocal/glib.m4
