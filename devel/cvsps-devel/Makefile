PORTNAME=	cvsps
PORTVERSION=	1.3.3
CATEGORIES=	devel
MASTER_SITES=	http://www.cobite.com/cvsps/

MAINTAINER=	ports@FreeBSD.org

USE_GMAKE=	yes
ALL_TARGET=	cvsps

MAN1=	cvsps.1

do-install:
	${INSTALL_PROGRAM} ${WRKSRC}/cvsps ${PREFIX}/bin
	${INSTALL_MAN} ${WRKSRC}/cvsps.1 ${MANPREFIX}/man/man1

.include <bsd.port.mk>
