bin/ytree
@unexec if cmp -s %D/etc/ytree.conf %D/etc/ytree.conf.sample; then rm -f %D/etc/ytree.conf; fi
etc/ytree.conf.sample
@exec [ -f %B/ytree.conf ] || cp %B/%f %B/ytree.conf
