@group wheel
@exec [ -f %D/etc/upclient.conf ] || cp -p %D/etc/upclient.conf.sample %D/etc/upclient.conf
man/man8/upclient.8.gz
sbin/clientchk
sbin/upchk
@group kmem
sbin/upclient
@group wheel
