      <url>http://www.osvdb.org/7870</url>
      <url>http://www.osvdb.org/7871</url>
      <cvename>CAN-2004-0594</cvename>
      <cvename>CAN-2004-0595</cvename>
    </references>
    <dates>
      <discovery>2007-07-07</discovery>
      <entry>2004-07-15</entry>
    </dates>
  </vuln>

