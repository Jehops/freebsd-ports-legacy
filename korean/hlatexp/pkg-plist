@comment modify texmf.cnf
@exec perl -pi -e 's@(^PKFONTS\s+=\s+\.)@\1;\$TEXMF/fonts/pk/\{xhan300,xhan600\}@g' %D/share/texmf/web2c/texmf.cnf
