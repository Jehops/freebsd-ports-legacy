-us-distdir: distdir dist-hook
-	mv $(distdir) $(distdir)-us
-	find $(distdir)-us -type f -print | while read f ; do 		\
-		cp $$f `dirname $$f`/_`basename $$f`.$$$$ &&   		\
-		rm $$f && mv `dirname $$f`/_`basename $$f`.$$$$ $$f ;	\
-	done
-
-us-dist: us-distdir
