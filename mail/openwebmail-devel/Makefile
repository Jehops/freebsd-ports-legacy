MASTERDIR=	${.CURDIR}/../openwebmail
MD5_FILE=	${.CURDIR}/distinfo

CONFLICTS=	${PORTNAME}-[0-9].[0-9][0-9]

.include "${MASTERDIR}/Makefile"
