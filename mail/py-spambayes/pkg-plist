@cwd /
@exec mkdir -p %%CACHEDIR%%/pop3proxy-spam-cache
@exec mkdir -p %%CACHEDIR%%/pop3proxy-ham-cache
@exec mkdir -p %%CACHEDIR%%/pop3proxy-unknown-cache
@exec mkdir -p %%DBDIR%%
@dirrmtry %%CACHEDIR%%/pop3proxy-spam-cache
@dirrmtry %%CACHEDIR%%/pop3proxy-ham-cache
@dirrmtry %%CACHEDIR%%/pop3proxy-unknown-cache
@dirrmtry %%CACHEDIR%%
@dirrmtry %%DBDIR%%
@dirrmtry %%LOCALSTATDIR%%
@cwd %%RESETPREFIX%%
