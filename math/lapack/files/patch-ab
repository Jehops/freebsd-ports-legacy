+ #include <sys/types.h>
+ #include <sys/time.h>
+ #include <sys/resource.h>
+ 
+ float
+ etime_ (float *a)
+ {
+     struct rusage r;
+     float f;
+ 
+     getrusage(RUSAGE_SELF, &r);
+     f = r.ru_utime.tv_sec + (float) r.ru_utime.tv_usec/1000000;
+     *a = f;
+     return f;
+ }
