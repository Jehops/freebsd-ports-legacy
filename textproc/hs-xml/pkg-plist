@exec /bin/sh %D/%%HSXML_LIBDIR_REL%%/register.sh
@exec /bin/rm -f %D/lib/ghc-%%GHC_VERSION%%/package.conf.old
@unexec %D/bin/ghc-pkg unregister xml
@unexec /bin/rm -f %D/lib/ghc-%%GHC_VERSION%%/package.conf.old
