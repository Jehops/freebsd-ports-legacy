PORTNAME=	asm2html
PORTVERSION=	1.4
CATEGORIES=	textproc
MASTER_SITES=	http://virtus.ath.cx/asm2html/ \
		http://asm2html.kerneled.com

MAINTAINER=	tuxsuximus@hotmail.com
COMMENT=	Converts NASM syntax assembly code to HTML code

MAN1=		asm2html.1
HAS_CONFIGURE=	yes
CONFIGURE_ARGS+=--prefix=${PREFIX}

