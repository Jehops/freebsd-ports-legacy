@@ -259,10 +262,13 @@
 (define (gentext-ca-intra-label-sep gind)
   (let* ((giname (if (string? gind) gind (gi gind)))
 	 (name   (normalize giname))
+	 (lsep   (assoc name (local-ca-intra-label-sep)))
 	 (sep    (assoc name (ca-intra-label-sep))))
-    (if sep
-	(car (cdr sep))
-	"")))
+    (if lsep
+        (car (cdr lsep))
+        (if sep
+            (car (cdr sep))
+            ""))))
 
