+  @set %LDSHR_Footer          = "LDSHR_Footer";
+  @set %LDSHR_ArchiveFullPath = "LDSHR_ArchiveFullPath";
+
+  @template LDSHR_ArchiveFullPath ( %LibDir, %LibName ) is
+   $\^
+  @end;
+
+  @template LDSHR_Header ( %Target, %LDSHR_DBMSOpt, %CSF_LINKER_SysShared ) is
