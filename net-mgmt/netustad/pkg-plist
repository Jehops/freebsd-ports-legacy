etc/rc.d/netustad.sh
@unexec if cmp -s %D/etc/netustad.conf %D/etc/netustad.conf-dist; then rm -f %D/etc/netustad.conf; fi
etc/netustad.conf-dist
@exec [ -f %B/netustad.conf ] || cp %B/%f %B/netustad.conf
