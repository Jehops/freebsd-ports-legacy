#!/bin/sh
# $FreeBSD$

# PROVIDE: sulci
# REQUIRE: DAEMON

# Define these sulci_* variables in one of these files:
#       /etc/rc.conf
#       /etc/rc.conf.local
#       /etc/rc.conf.d/sulci
#
# sulci_enable : bool
#   Enable ("YES") or disable ("NO", the default) this startup script.
#
# sulci_precmd : string
#   Execute this command before the daemon startup, ":" by default.
#

. %%RC_SUBR%%

name="sulci"
rcvar=`set_rcvar`

command="%%PREFIX%%/sbin/sulci"
command_args="&"

load_rc_config "${name}"

: ${sulci_enable="NO"}
: ${sulci_precmd=":"}

start_precmd="${sulci_precmd}"

run_rc_command "$1"
