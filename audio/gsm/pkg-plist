bin/toast
@exec ln -sf %F %B/tcat
@exec ln -sf %F %B/untoast
@unexec rm %D/bin/tcat
@unexec rm %D/bin/untoast
include/gsm.h
lib/libgsm.a
lib/libgsm.so
lib/libgsm.so.1
@exec /usr/bin/env OBJFORMAT=%%PORTOBJFORMAT%% /sbin/ldconfig -m %B
@unexec /usr/bin/env OBJFORMAT=%%PORTOBJFORMAT%% /sbin/ldconfig -R
