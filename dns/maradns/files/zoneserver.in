#!/bin/sh
#
# $FreeBSD$
#

# PROVIDE: zonserver
# REQUIRE: SERVERS
# BEFORE: DAEMON
# KEYWORD: shutdown
#

. %%RC_SUBR%%

name="zonserver"
rcvar=`set_rcvar`
command="%%PREFIX%%/bin/duende"
pidfile="/var/run/${name}.pid"

load_rc_config $name

zonserver_enable=${zonserver_enable-"NO"}

run_rc_command "$1"
