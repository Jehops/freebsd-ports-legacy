  SUBDIRS = port common image html plain proto mxw chimera
  
  MakeSubdirs($(SUBDIRS))
! InstallSubdirs(main)
  DependSubdirs($(SUBDIRS))
  CleanSubdirs($(SUBDIRS))
  MakefileSubdirs($(SUBDIRS))
! InstallManSubdirs(main)
  
