%%FASTCGI_LIBDIR_REL%%/register.sh
%%NOPORTDOCS%%%%DOCSDIR%%/LICENSE
%%NOPORTDOCS%%@dirrmtry %%DOCSDIR%%
@dirrm %%FASTCGI_LIBDIR_REL%%/ghc-%%GHC_VERSION%%/Network
