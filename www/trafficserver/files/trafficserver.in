#!/bin/sh
#
# $FreeBSD$
#

# PROVIDE: trafficserver
# REQUIRE: LOGIN
# KEYWORD: shutdown

. /etc/rc.subr

name="trafficserver"
pidfile="/var/run/${name}.pid"
command="/usr/sbin/daemon -p ${pidfile} %%PREFIX%%/bin/traffic_server"
procname="%%PREFIX%%/bin/traffic_server"

load_rc_config "$name"

: ${trafficserver_enable="NO"}

run_rc_command "$1"
