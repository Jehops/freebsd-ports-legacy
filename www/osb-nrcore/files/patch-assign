+KWQDictImpl &KWQDictImpl::assign(const KWQDictImpl &di, bool deleteItems)
+{
+    KWQDictImpl tmp(di);
+    
+    if (deleteItems) {
+	clear(true);
+    }
+
+    swap(tmp);
+
+    return *this;
+}
