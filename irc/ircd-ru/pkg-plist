@dirrm etc/ircd-ru/ru_RU.CP1251
@dirrm etc/ircd-ru/ru_RU.KOI8-R
@dirrm share/ircd-ru/codepages
@dirrm share/ircd-ru/modules
@dirrm share/ircd-ru/
@dirrm share/doc/ircd-ru/ru_RU.KOI8-R
@dirrm share/doc/ircd-ru
@exec mkdir -m 755 %%VARDIR%%/log/ircd-ru
@exec chown ircd:ircd %%VARDIR%%/log/ircd-ru
@exec mkdir -m 755 %%VARDIR%%/run/ircd-ru
@exec chown ircd:ircd %%VARDIR%%/run/ircd-ru
@unexec rm -R %%VARDIR%%/run/ircd-ru > /dev/null
@unexec rm -R %%VARDIR%%/log/ircd-ru > /dev/null
@unexec rm /usr/local/etc/rc.d/ircd.sh
@unexec echo 'To completely remove this port delete user ircd, group ircd and remove etc/ircd-ru'
