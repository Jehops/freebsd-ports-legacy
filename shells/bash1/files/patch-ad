*** 90,101 ****
  
  installdirs:
  	-[ -d $(mandir) ] || mkdir $(mandir)
! 	-[ -d $(man3dir) ] || mkdir $(man3dir)
  	-[ -d $(infodir) ] || mkdir $(infodir)
  
  install: all installdirs
  	$(INSTALL_DATA) bash.1 $(mandir)
! 	$(INSTALL_DATA) readline.3 $(man3dir)
  	$(INSTALL_DATA) features.info $(infodir)/bash.info
  
  uninstall:
