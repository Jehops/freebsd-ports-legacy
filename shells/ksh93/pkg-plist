bin/ksh93
@exec echo "Updating /etc/shells"; sed -i "" "\|%D/%F|d" /etc/shells; echo %D/%F >> /etc/shells
@unexec echo "Updating /etc/shells"; sed -i "" "\|%D/%F|d" /etc/shells
