@exec mkdir /var/adm/tac_plus/allow
@exec mkdir /var/adm/tac_plus/parrot
@dirrm tac_plus/deny
@dirrm tac_plus/allow
@dirrm tac_plus/parrot
@dirrm tac_plus
@cwd /var/log
@unexec rmdir %D/tac_plus 2>/dev/null || true
