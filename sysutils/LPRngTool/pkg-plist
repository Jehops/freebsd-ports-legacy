@unexec if cmp -s %D/%%SYSCONFDIR%%/lprngtool.conf %D/%%SYSCONFDIR%%/lprngtool.conf.sample; then rm -f %D/%%SYSCONFDIR%%/lprngtool.conf; fi
%%SYSCONFDIR%%/lprngtool.conf.sample
@exec [ -f %B/lprngtool.conf ] || cp %B/%f %B/lprngtool.conf
