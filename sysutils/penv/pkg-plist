bin/penv
@unexec if cmp -s %D/etc/penv.conf %D/etc/penv.conf.default; then rm -f %D/etc/penv.conf; fi
etc/penv.conf.default
@exec [ -f %B/penv.conf ] || cp %B/%f %B/penv.conf
