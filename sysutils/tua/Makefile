DISTNAME=       tua-4.0
MASTER_SITES=   ftp://itnsg1.cineca.it/local/tua/
GNU_CONFIGURE=	YES

pre-install:
	mkdir -p ${PREFIX}/bin
	mkdir -p ${PREFIX}/man/man1
	mkdir -p ${PREFIX}/info
	mkdir -p ${PREFIX}/lib

.include <bsd.port.mk>
